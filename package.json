{
  "name": "natours",
  "version": "1.0.0",
  "description": "landing page for natours",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefix -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "build:css": "npm-run-all compile:sass concat:css "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Belindaliang51/advanced_CSS_and_Sass.git"
  },
  "author": "Belinda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Belindaliang51/advanced_CSS_and_Sass/issues"
  },
  "homepage": "https://github.com/Belindaliang51/advanced_CSS_and_Sass#readme",
  "dependencies": {
    "live-server": "^1.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "concat": "^1.0.3",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.0"
  }
}
